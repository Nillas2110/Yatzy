let DVALUE=0
let PLAYERCOUNT=[1,2,3,4,5,6];
let DAMOUNT=(4,5,6)


function setup() {
  createCanvas(400, 400);
}
function mousePressed(){
  if(mouseX<width*0.75 && mouseX>width*0.25 && mouseY>height*0.55 && mouseY<height*0.55+height*0.15){
  DVALUE=floor(random(1,7));
  console.log("dice rolled")
  }
  }
function draw() {
  background(220);
  rect(width/4,height*0.55,width/2,height*0.15);
  text(DVALUE,width/2,height/2);
  text("roll dice",width*0.45,height*0.63)
}
